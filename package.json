{
  "name": "workouts6-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend for CrossFit Workout Display System",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "node --loader ts-node/esm server/index.ts",
    "build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "node dist/server/index.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["crossfit", "workout", "display"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "pg": "^8.11.3",
    "esbuild": "^0.20.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6"
  }
}